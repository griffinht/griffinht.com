<html>
<head>
    {{> ../head}}
</head>
<body>
  <article>
    <h1>{{title}}</h1>
    <date datetime="{{datetime}}">{{datetime}}</date>
    {{#sections}}
    <section>
      {{#header}}<h2>{{.}}</h2>{{/header}}
      {{#img}}<img src="{{root}}{{.}}">{{/img}}
      {{content}}
    </section>
    {{/sections}}
  </article>
  {{> ../footer}}
</body>
</html>
